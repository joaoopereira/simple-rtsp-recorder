{
  "name": "simple-rtsp-recorder",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env ENV_FILE=.env node server.js",
    "pack": "rimraf dist && pkg ."
  },
  "bin": {
    "simple-rtsp-recorder": "server.js"
  },
  "pkg": {
    "scripts": "server.js",
    "assets": "prod.env",
    "targets": [ "latest-win-x64", "latest-linux-x64" ],
    "outputPath": "dist"
  },
  "keywords": [
    "rtsp",
    "recorder"
  ],
  "author": "joaoopereira",
  "license": "None",
  "description": "simple web server to record file from an rtsp camera stream",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "fluent-ffmpeg": "^2.1.3",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "pkg": "5.8.1",
    "rimraf": "^6.0.1"
  }
}
